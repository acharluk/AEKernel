local _build = '20150506.1651'
local _version = '0.1'

local _args = {}

local _workingDirectory = '/AEKernel/'
local _ApiPath = _workingDirectory..'apis/'
local _requiredApis = {
  'akfs'
}

--[[ Init, dispose, other functions ]]--
initialise = function(self, args)
  for i = 1, #_requiredApis do
  print('loading: ' .. _ApiPath .. _requiredApis[i])
    if not os.loadAPI(_ApiPath .. _requiredApis[i]) then
      error('The API: ' .. _requiredApis[i] .. ' could not be loaded.')
    end
  end  
  _args = args
  return self
end

dispose = function(self)
  for i = 1, #_requiredApis do
    if not os.unloadAPI(_ApiPath .. _requiredApis[i]) then
      error('The API: ' .. _requiredApis[i] .. ' could not be unloaded.')
    end
  end
end

--[[ AKFS functions ]]--



